@using CMS.MHH.ViewModel
@model List<Export_IdeaVM>
@{
    ViewBag.Title = "Files";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table">
    <tr>
        <th>Content</th>
        <th></th>
    </tr>
    @foreach(var item in Model)
    {
        <tr>
            <td>@item.Content</td>
            <td>@Html.ActionLink("Export", "Export", new { Id= item.SubmissionId})</td>
        </tr>
    }

</table>



